{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nombre de instancia de Data Factory",
			"defaultValue": "BackupOrigenDestinoTest"
		},
		"AzureBlobStorageBck_connectionString": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"connectionString\"de \"AzureBlobStorageBck\""
		},
		"AzureBlobStorageOrigen_connectionString": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"connectionString\"de \"AzureBlobStorageOrigen\""
		},
		"Trigger_iom_properties_pipeline_parameters_windowStart": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowStartTime"
		},
		"Trigger_iom_properties_pipeline_parameters_windowEnd": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowEndTime"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI de la cuenta de almacenamiento que contiene las plantillas de ARM vinculadas."
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Token de SAS para la cuenta de almacenamiento que contiene las plantillas de ARM vinculadas"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "BackupOrigenDestinoTest_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorageBck_connectionString": {
						"value": "[parameters('AzureBlobStorageBck_connectionString')]"
					},
					"AzureBlobStorageOrigen_connectionString": {
						"value": "[parameters('AzureBlobStorageOrigen_connectionString')]"
					},
					"Trigger_iom_properties_pipeline_parameters_windowStart": {
						"value": "[parameters('Trigger_iom_properties_pipeline_parameters_windowStart')]"
					},
					"Trigger_iom_properties_pipeline_parameters_windowEnd": {
						"value": "[parameters('Trigger_iom_properties_pipeline_parameters_windowEnd')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}